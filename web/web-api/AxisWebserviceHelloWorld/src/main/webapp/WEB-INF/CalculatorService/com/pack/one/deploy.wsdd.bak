<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from CalculatorService WSDL service -->

  <service name="Calculator" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://one.pack.com"/>
      <parameter name="wsdlServiceElement" value="CalculatorService"/>
      <parameter name="schemaQualified" value="http://one.pack.com"/>
      <parameter name="wsdlServicePort" value="Calculator"/>
      <parameter name="className" value="com.pack.one.CalculatorSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Calculator"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="sum" qname="operNS:sum" xmlns:operNS="http://one.pack.com" returnQName="retNS:sumReturn" xmlns:retNS="http://one.pack.com" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <parameter name="allowedMethods" value="sum"/>

  </service>
</deployment>
